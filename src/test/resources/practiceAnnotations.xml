<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Annotations">
    <test name="Smoke">
        <groups>
            <run>
                <include name="smoke"></include>
            </run>
        </groups>
        <classes>
            <class name="tests.PracticeDependOnTests"></class>
        </classes>
    </test>
    <test name="Regression">
        <classes>
            <class name="tests.PracticeDependOnTests"></class>
        </classes>
    </test>
</suite>